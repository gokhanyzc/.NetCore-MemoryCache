@model TeamSquad
@{
    ViewData["Title"] = "Home Page";
}

<table>
    <thead>
        <h1> CheckTime is @Model.CheckTime.ToString()</h1>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    @foreach (var player in Model.Players)
    {
        <tr>
            <td>@player.Id</td>
            <td>@player.Name</td>
        </tr>
    }
</table>